/* -------登录的弹窗------------*/

.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
  display: none;
}

.mask2 {
  display: block;
}

.container {
  position: fixed;
  bottom: 480rpx;
  width: 550rpx;
  height: 360rpx;
  margin-left: 100rpx;
  /* transform: translateY(300%);
  transition: all 0.4s ease; */
  z-index: 12;
  border-radius: 20rpx;
}

.dialog .container .prop {
  position: relative;
}

.dialog .container .prop image {
  display: block;
  width: 46rpx;
  height: 46rpx;
  padding: 10rpx;
  position: absolute;
  top: -30rpx;
  right: -30rpx;
}

.dialog .container .prop .coarse {
  width: 180rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 50rpx;
  padding-top: 60rpx;
  margin: 0 auto;
}

.dialog .container .prop .thin {
  width: 260rpx;
  height: 37rpx;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 37rpx;
  padding-top: 36rpx;
  margin: 0 auto;
}

.dialog .container .prop .prop-button {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  width: 450rpx;
  margin: 0 auto;
  margin-top: 60rpx;
}

.dialog .container .prop .prop-button .ok,
.dialog .container .prop .prop-button .no {
  width: 128rpx;
  height: 45rpx;
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 400;
  line-height: 45rpx;
  padding: 12rpx 39rpx;
  border-radius: 35rpx;
}

.dialog .container .prop .prop-button .ok {
  color: rgba(58, 141, 92, 1);
  background: rgba(255, 255, 255, 1);
}

.dialog .container .prop .prop-button .no {
  color: rgba(255, 255, 255, 1);
  border: 2rpx solid rgba(255, 255, 255, 1);
}
