page{
  height:100%;
  background:rgba(240,240,240,1);
}

.demand {
  padding: 0 24rpx;
  padding-top: 56rpx;
  background:rgba(240,240,240,1);
}
.demand .row1{
  padding-left:20rpx;
  padding-top: 19rpx;
  background-color: #fff;
  border-radius:12rpx;
}

.demand .row1 .inmore{
  height: 286rpx;
  font-size:30rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:#333;
  width: 100%;
}

.ph{
  height:42rpx;
  font-size:30rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(153,153,153,1);
  line-height:42rpx;
}
#hint {
  height:33rpx;
  font-size:24rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  line-height:33rpx;
  margin-top: 20px;
}
#hint>text {
  color: #edaf33;
  font-size: 26rpx;
}

.kjbq{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(153,153,153,1);
  line-height:37rpx;
  margin-top:30rpx;
  padding-bottom: 26rpx;
}

#kj-list .wb{
  height:33rpx;
  font-size:24rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(102,102,102,1);
  line-height:33rpx;
  padding-left: 16rpx;
  border-radius:7px;
  border:1rpx solid rgba(151,151,151,1);
  margin-bottom: 30rpx;
  display: inline-block;
}
#kj-list .wb1{
  width: 420rpx;
}
#kj-list .wb2{
  width: 414rpx;
}
#kj-list .wb3{
  width: 302rpx;
}
#kj-list .wb4{
  width: 278rpx;
  margin-left: 10rpx;
}
#kj-list .wb5{
  width: 372rpx;
}
#kj-list .wb6{
  width: 270rpx;
  margin-left: 10rpx;
}
#kj-list .wb7{
  width: 270rpx;
}

#kj-list>input:hover{
  border-color:rgba(145,213,173,1);
  color:rgba(145,213,173,1);
}

.qran{
  margin-top:64rpx;
}
.qran button{
  width:560rpx;
  height:90rpx;
  line-height: 90rpx;
  background:rgba(145,213,173,1);
  border-radius:45rpx;
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
}

.qran button::after{
  border: none;
}