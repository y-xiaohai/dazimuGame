page{
  background: whitesmoke;
}
.head swiper{
  height: 406rpx;
  background: whitesmoke;
}
swiper-item>image {
  width: 100%;
  height: 100%;
  background: whitesmoke;
}

/* --------- */
.midd{
  background: #fff;
  padding-top: 36rpx;
  padding-bottom: 30rpx;
}
.pub{
  margin: 0 24rpx;
}
.title{
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:50rpx;
}
.content{
  margin-top: 18rpx;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(153,153,153,1);
  line-height:42rpx;
}
.price{
  margin-top: 16rpx;
  height:59rpx;
  font-size:42rpx;
  font-family:PingFangSC-Medium;
  font-weight:500;
  color:rgba(237,175,51,1);
  line-height:59rpx;
}
.prompt{
  margin-top: 20rpx;
  font-size:24rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(145,213,173,1);
  line-height:38rpx;
}

.fgx{
  width:750rpx;
  height:20rpx;
  background:rgba(244,244,244,1);
}

/* --------- */
.foot{
  background:#fff;
  padding: 30rpx 24rpx;
}
.foot .foot-top{
  display: flex;
  align-items: center;
  justify-content: start;
}
.foot .foot-bot{
  display: flex;
  align-items: flex-start;
  justify-content: space-between;
  margin-top: 22rpx;
}
.foot .ico{
  width:30rpx;
  height:30rpx;
  line-height: 37rpx;
}
.foot .gong {
  display: inline-block;
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(102,102,102,1);
  line-height:37rpx;
}
.foot .foot-top .time,.foot .foot-bot .address{
  margin-left: 18rpx;
}
.foot .foot-bot .address{
  display: inline-block;
  width: 460rpx;
  font-size:26rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(102,102,102,1);
  line-height:37rpx;
}
.foot .foot-bot .left{
  display: flex;
  align-items: flex-start;
  justify-content: start;
}
.foot .foot-bot .distance{
  color:rgba(145,213,173,1);
}

/* ------------ */
.complaint{
  width:750rpx;
  background: whitesmoke;
  padding: 35rpx 0;
}
.complaint .tsdd{
  width:88rpx;
  height:30rpx;
  font-size:22rpx;
  font-family:PingFangSC-Light;
  font-weight:300;
  color:rgba(153,153,153,1);
  line-height:30rpx;
  border-radius:30rpx;
  border:1px solid rgba(187,187,187,1);
  padding: 6rpx 20rpx;
  margin: 0 auto;
}

.after .conf{
  width: 750rpx;
  text-align: center;
  height:100rpx;
  font-size:36rpx;
  font-family:PingFangSC-Regular;
  background:rgba(145,213,173,1);
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:100rpx;
  /* padding-top: 25rpx;
  padding-bottom: 25rpx; */
  position: fixed;
  left: 0;
  bottom: 0;
  border-radius: 0rpx;
}

.after button::after{
  border: none;
}

/* -------弹窗------------*/
.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
  display: none;
}

.mask2 {
  display: block;
}

.container {
  position: fixed;
  bottom: 480rpx;
  width: 550rpx;
  height: 360rpx;
  margin-left: 100rpx;
  /* transform: translateY(300%);
  transition: all 0.4s ease; */
  z-index: 12;
  border-radius: 20rpx;
}

.dialog .container .prop{
  position: relative;
}

.dialog .container .prop image{
  display: block;
  width: 46rpx;
  height: 46rpx;
  padding: 10rpx;
  position: absolute;
  top: -30rpx;
  right: -30rpx;
}

.dialog .container .prop .coarse{
  width:180rpx;
  height:50rpx;
  font-size:36rpx;
  font-family:PingFangSC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:50rpx;
  padding-top: 60rpx;
  margin: 0 auto;
}

.dialog .container .prop .thin{
  width:260rpx;
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:37rpx;
  padding-top: 36rpx;
  margin: 0 auto;
}

.dialog .container .prop .prop-button{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  width: 450rpx;
  margin: 0 auto;
  margin-top: 60rpx;
}

.dialog .container .prop .prop-button .ok,
.dialog .container .prop .prop-button .no{
  width:128rpx;
  height:45rpx;
  font-size:32rpx;
  font-family:PingFangSC;
  font-weight:400;
  line-height:45rpx;
  padding: 12rpx 39rpx;
  border-radius:35rpx;
}

.dialog .container .prop .prop-button .ok{
  color:rgba(58,141,92,1);
  background:rgba(255,255,255,1);
}

.dialog .container .prop .prop-button .no{
  color:rgba(255,255,255,1);
  border:2rpx solid rgba(255,255,255,1);
}