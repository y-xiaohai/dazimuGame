  <view class="mask {{showDialog?'mask2':''}}" catchtouchmove="preventTouchMove"></view>
  <view class="container">
    <view class="prop">
      <image src="/images/icon1.png" catchtap='close' catchtouchmove='preventTouchMove'></image>
