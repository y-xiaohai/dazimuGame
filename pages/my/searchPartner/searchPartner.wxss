page{
  background: #f7f7f7;
}

.search-box{
  padding: 36rpx 24rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  color: #333;
}

.search-box .inp{
  display: flex;
  flex-direction: row;
  align-items: center;
  background:rgba(240,240,240,1);
  border-radius: 10rpx;
  padding: 16rpx 26rpx;
}

.search-box .inp image{
  width:24rpx;
  height:24rpx;
}

.search-box .inp input{
  width: 430rpx;
  height:40rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
  padding-left: 14rpx;
}

.search-box .search{
  width:56rpx;
  height:40rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(237,175,51,1);
  line-height:40rpx;
  border-radius:10rpx;
  border:1px solid rgba(237,175,51,1);
  padding: 16rpx 40rpx;
}

.line{
  width:750rpx;
  height:2rpx;
  background:rgba(237,237,237,1);
  border-radius:1rpx;
}

/* ------------- */
.lists{
  background: #fff;
  /* background: green; */
  margin: 0 24rpx;
  margin-top: 20rpx;
  border-radius: 8rpx;
  padding-bottom: 20rpx;
}
.lists .lists-out{
  /* background: #fff; */
  /* background: gold; */
  /* margin: 0rpx 24rpx; */
  padding: 27rpx 24rpx 30rpx 24rpx;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
  border-bottom: 1rpx solid #EDEDED;
}
.lists .more{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:37rpx;
  text-align: center;
  margin-top: 20rpx;
}
.lists .lists-out .top-row{
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
}
.lists .lists-out .top-row .par-img{
  width:70rpx;
  height:70rpx;
  border:2rpx solid rgba(79,176,198,1);
  border-radius: 50%;
  display: inline-block;
  /* background: greenyellow; */
}

.lists .lists-out .top-row .par-level .color1{
  background: #999999;
}
.lists .lists-out .top-row .par-level .color2{
  background: #DE6549;
}
.lists .lists-out .top-row .par-level .color3{
  background: #E97F00;
}
.lists .lists-out .top-row .par-level .color4{
  background: #FBD14A;
}
.lists .lists-out .top-row .par-level .color5{
  background: #8CD790;
}
.lists .lists-out .top-row .par-level .color6{
  background: #4FB0C6;
}
.lists .lists-out .top-row .par-level .color7{
  background: #4E86C6;
}
.lists .lists-out .top-row .par-level .color8{
  background: #6C49B8;
}

.lists .lists-out .top-row .par-name{
  height:45rpx;
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:45rpx;
  margin-left: -60rpx;
}
.lists .lists-out .top-row .par-score{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:37rpx;
}

.lists .lists-out .top-row .par-score .othercolor{
  color: #63B596;
}
.lists .lists-out .top-row .par-level{
  /* display: flex;
  flex-direction: row;
  align-items: center; */
  position: relative;
}
.lists .lists-out .top-row .par-level .parlevel-img{
  display: block;
  width: 52rpx;
  height: 52rpx;
  /* border: 4rpx solid red; */
  border-radius: 50%;
  position: absolute;
  left: -2rpx;
  /* background: yellow; */
  bottom: -5rpx;
}

.lists .lists-out .top-row .par-level .parlevel-name{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:37rpx;
  padding: 3rpx 18rpx 2rpx 62rpx;
  /* background: rgba(108, 73, 184, 1); */
  border-radius: 21rpx;
}

.lists .lists-out .bottom-row{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 22rpx;
}
.lists .lists-out .bottom-row text{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:37rpx;
}
.lists .lists-out .bottom-row .put{
  margin-right: 68rpx;
}
.lists .lists-out .bottom-row .pec{
  color: #63B596;
}


