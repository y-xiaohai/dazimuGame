}

.not image{
  width: 46rpx;
  height: 52rpx;
  display: block;
  margin: 0 auto;
  margin-bottom: 15rpx;
}

.not .wenzi{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:40rpx;
  letter-spacing:1px;
}

