  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 28rpx;
  /* background: blue; */
}

.out2 text{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFangSC;
