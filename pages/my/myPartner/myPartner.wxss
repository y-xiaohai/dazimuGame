#head {
  overflow: hidden;
  padding: 52rpx 24rpx 84rpx 24rpx;
}

.out {
  width: 702rpx;
  display: flex;
  flex-direction: row;
}

.out .left {
  width: 116rpx;
  height: 116rpx;
}

.out .left image {
  width: 112rpx;
  height: 112rpx;
  border: 2rpx solid rgba(255, 255, 255, 1);
  border-radius: 50%;
  display: block;
}

.out .right {
  width: 554rpx;
  height: 116rpx;
  margin-left: 32rpx;
}

.out .right .row1 {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items: center;
  margin-top: 6rpx;
}

.out .right .row1 .name {
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 50rpx;
}

.out .right .row1 .team {
  height: 42rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 42rpx;
}

.out .right .row1 .team text {
  display: inline-block;
}

.out .right .row1 .team .other {
  color: #396c4e;
}

.out .right .row1 .level{
  /* display: flex;
  flex-direction: row;
  align-items: center; */
  position: relative;
}
.out .right .row1 .level .level-img {
  width: 52rpx;
  height: 52rpx;
  /* border: 4rpx solid #fff; */
  border-radius: 50%;
  position: absolute;
  left: -2rpx;
  display: block;
  bottom: -5rpx;
}

.out .right .row1 .level .level-name {
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:37rpx;
  padding: 3rpx 18rpx 2rpx 62rpx;
  /* background: rgba(108, 73, 184, 1); */
  border-radius: 21rpx;
}

