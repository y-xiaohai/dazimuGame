.out .right .row2{
  display: flex;
  flex-direction: row;
  margin-top: 14rpx;
}

.out .right .row2 .left-jf text,
.out .right .row2 .right-jf text{
  height:37rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:37rpx;
}

.out .right .row2 .right-jf{
  margin-left: 66rpx;
}

.out .right .row2 .left-jf .other,
.out .right .row2 .right-jf .other{
