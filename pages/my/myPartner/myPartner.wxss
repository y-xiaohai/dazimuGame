/* ---------------- */
#members{
  position: absolute;
  width: 100%;
  top: 293rpx;
  border-radius:16rpx 16rpx 0rpx 0rpx;
}

.navout{ 
  background: #FFF; 
  /* background: red; */
  text-align: center;
  border-radius:16rpx 16rpx 0rpx 0rpx;
  width: 100%;
  height: 100rpx;
} 

.navout .item{ 
  display: inline-block;
  width: 50%;
  height:45rpx;
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:45rpx;
  padding: 28rpx 0;
} 

/* 顶部导航点击后样式 */
.navout .item.active{ 
  height:45rpx;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFangSC;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:45rpx;
} 

/* 顶部指示条属性 */
.navout .item.active:after{ 
  content: ""; 
  display: block;
  height: 6rpx; 
  width:128rpx;
  background:rgba(145,213,173,1);
  border-radius:3rpx;
  margin: 0 auto;
  margin-top: 17rpx;
}

.tree .inviter{
  height:42rpx;
  font-size:30rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:42rpx;
  margin-left: 46rpx;
  margin-top: 36rpx;
}

.tree .myself{
  height: 120rpx;
  margin-top: 36rpx;
  text-align: center;
}
.tree .myself image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.tree .slash .slash-in{
  width: 242rpx;
  height: 61rpx;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
}

.tree .slash .slash-in .imgview{
  width: 121rpx;
  height: 61rpx;
  /* background: red; */
}

.tree .slash .slash-in .imgview image{
  width: 61rpx;
  height: 61rpx;
}

/* ----- */
.tree .tree-wall{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  margin-bottom: 36rpx;
  position: relative;/*用于设置搜索悬窗 */
}
.tree .tree-wall .half{
  /* background: white; */
  width: 50%;
}

.tree .tree-wall .half .half-top{
  /* background: red; */
  width: 208rpx;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 10rpx 0;
  margin-left: 46rpx;
}

.tree .tree-wall .half .half-top .name{
  width:72rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFangSC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:34rpx;
}

.tree .tree-wall .half .half-top .name>view{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.tree .tree-wall .half .half-top .img-icon>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  display: block;
}

.tree .tree-wall .half .half-foot{
  width: 4rpx;
  height: 40rpx;
  margin-left: 202rpx;
  background:rgba(102,102,102,1);
}

